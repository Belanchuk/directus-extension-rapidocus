{
  "name": "directus-extension-rapidocus",
  "description": "View interactive API documentation from OpenAPI Specification of your Directus and customize the appearance.",
  "icon": "temp_preferences_eco",
  "version": "1.0.0",
  "keywords": [
    "openapi",
    "rapidoc",
    "directus",
    "directus-extension",
    "directus-extension-module"
  ],
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "module",
    "path": "dist/index.js",
    "source": "src/index.js",
    "host": "^10.10.0",
    "sandbox": {
      "enabled": true,
      "requestedScopes": {}
    }
  },
  "scripts": {
    "dev": "directus-extension build -w --no-minify --sourcemap",
    "build": "directus-extension build",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "11.0.4",
    "vue": "^3.4.27"
  },
  "dependencies": {
    "rapidoc": "^9.3.4"
  }
}
